{
  "name": "guide4you-module-urlapi",
  "version": "1.2.3",
  "description": "An urlapi module for Guide4You.",
  "keywords": [
    "urlapi",
    "map",
    "map client",
    "openlayers"
  ],
  "author": "Klaus Benndorf",
  "homepage": "https://github.com/KlausBenndorf/guide4you-module-urlapi",
  "contributors": [
    "Sascha Klemenjak <sascha.klemenjak@benndorf.de>",
    "Josef Schugt <josef.schugt@benndorf.de>",
    "Simon Seyock <simonseyock@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KlausBenndorf/guide4you-module-urlapi"
  },
  "dependencies": {
    "guide4you": "1.2.*",
    "guide4you-proxy": "github:KlausBenndorf/guide4you-proxy#v1.0.0"
  },
  "devDependencies": {
    "guide4you-builder": "1.1.*",
    "concurrently": "3.0.0",
    "esdoc": "0.4.8",
    "mocha": "3.1.0",
    "selenium-webdriver": "2.53.3",
    "phantomjs-prebuilt": "2.1.11",
    "standard": "8.2.0",
    "wait-on": "1.5.2"
  },
  "scripts": {
    "build": "g4u-build -m prod -c",
    "dev": "g4u-build -m dev -c",
    "lint": "eslint src/ tests/",
    "pretest": "g4u-buildTests",
    "runTests": "npm run lint && wait-on -l http-get://localhost:8080 && mocha build/tests/*",
    "test": "concurrently --kill-others --success first \"g4u-build -m dev -c conf\" \"npm run runTests\" && echo \"Tests completed successfully\"",
    "doc": "esdoc -c esdoc.json"
  }
}
